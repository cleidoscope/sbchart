{
  "name": "vue-sbchart",
  "description": "A simple baseline charting component for Vue 3",
  "version": "1.0.1",
  "private": false,
  "author": {
    "name": "Clyde Escobidal",
    "email": "cleidoscope@gmail.com"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-sbchart.umd.cjs",
  "module": "./dist/vue-sbchart.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
