  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "license": "MIT",
  "repository": "https://github.com/clydescobidal/vue-sbchart.git",
  "bugs": "https://github.com/clydescobidal/vue-sbchart/issues"
